---
const { origin } = Astro.props;
import Logo from './Logo.astro';
---


<header>
	<nav class="border-gray-200 bg-purple-900">
		<div
			class="max-w-screen-xl flex flex-wrap items-center justify-between mx-auto p-4"
		>
			<Logo name="For The Rock" />
			<button
				data-collapse-toggle="navbar-default"
				type="button"
				class="inline-flex items-center p-2 w-10 h-10 justify-center text-sm text-gray-500 rounded-lg md:hidden hover:bg-gray-300 focus:outline-none focus:ring-2 focus:ring-gray-200 dark:text-gray-400 dark:hover:bg-yellow-400 dark:focus:ring-gray-600"
				aria-controls="navbar-default"
				aria-expanded="false"
			>
				<span class="sr-only">Open main menu</span>
				<svg
					class="w-5 h-5"
					aria-hidden="true"
					xmlns="http://www.w3.org/2000/svg"
					fill="none"
					viewBox="0 0 17 14"
				>
					<path
						stroke="currentColor"
						stroke-linecap="round"
						stroke-linejoin="round"
						stroke-width="2"
						d="M1 1h15M1 7h15M1 13h15"></path>
				</svg>
			</button>
			<div class="hidden w-full md:block md:w-auto" id="navbar-default">
				<ul
					class="font-medium flex flex-col p-4 md:p-0 mt-4 border border-gray-100 rounded-lg md:flex-row md:space-x-8 rtl:space-x-reverse md:mt-0 md:border-0 dark:bg-gray-800 md:dark:bg-gray-900 dark:border-yellow-400 text-2xl"
				>
					<li>
						<a
							href="#"
							class="block py-2 px-3 text-white bg-yellow-300 rounded md:bg-transparent md:text-yellow-300 md:p-0 dark:text-white md:dark:text-yellow-300"
							aria-current="page">Home</a
						>
					</li>
					<li>
						<a
							href="/about"
							class="block py-2 px-3 text-gray-200 rounded hover:bg-yellow-500 md:hover:bg-transparent md:border-0 md:hover:text-yellow-300 md:p-0 dark:text-white md:dark:hover:text-yellow-300 dark:hover:bg-yellow-400 dark:hover:text-white md:dark:hover:bg-transparent"
							>About</a
						>
					</li>
					<li>
						<a
							href="/videos"
							class="block py-2 px-3 text-gray-200 rounded hover:bg-yellow-500 md:hover:bg-transparent md:border-0 md:hover:text-yellow-300 md:p-0 dark:text-white md:dark:hover:text-yellow-300 dark:hover:bg-yellow-400 dark:hover:text-white md:dark:hover:bg-transparent"
							>Videos</a
						>
					</li>
					<li>
						<a
							href="/music"
							class="block py-2 px-3 text-gray-200 rounded hover:bg-yellow-500 md:hover:bg-transparent md:border-0 md:hover:text-yellow-300 md:p-0 dark:text-white md:dark:hover:text-yellow-300 dark:hover:bg-yellow-400 dark:hover:text-white md:dark:hover:bg-transparent"
							>Music</a
						>
					</li>
					<li>
						<a
							href="/contact"
							class="block py-2 px-3 text-gray-200 rounded hover:bg-yellow-500 md:hover:bg-transparent md:border-0 md:hover:text-yellow-300 md:p-0 dark:text-white md:dark:hover:text-yellow-300 dark:hover:bg-yellow-400 dark:hover:text-white md:dark:hover:bg-transparent"
							>Contact</a
						>
					</li>
				</ul>
			</div>
		</div>
	</nav>

	
</header>
<style>
	header{
		font-family: 'Cinzel Decorative', system-ui;
	}
</style>